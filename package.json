    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dev-utils": "4.2.1",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "screencloud-ui-components": "git+ssh://git@github.com/screencloud/screencloud-ui-components.git",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
